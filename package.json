{
  "name": "@cuteribs/agent-context-viewer",
  "version": "0.1.0",
  "description": "Web application for analyzing Claude Code and Copilot CLI sessions",
  "type": "module",
  "bin": {
    "agent-context-viewer": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev -w server",
    "dev:client": "npm run dev -w client",
    "build": "node build.ts"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "open": "^10.0.3"
  }
}