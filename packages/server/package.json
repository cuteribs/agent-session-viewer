{
  "name": "@cuteribs/agent-session-viewer",
  "version": "0.1.2",
  "description": "Web application for analyzing Claude Code and Copilot CLI sessions",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent-session-viewer": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "build:public": "copyfiles -u 2 \"../client/dist/**/*\" dist/public",
    "prepublishOnly": "npm run build && npm run build:public",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "open": "^10.0.3",
    "shared": "*",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "copyfiles": "^2.4.1",
    "tsup": "^8.5.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}